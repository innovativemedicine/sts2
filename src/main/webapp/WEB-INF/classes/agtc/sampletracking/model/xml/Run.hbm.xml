<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 
    modified by Gloria Deng generate long mapping for longerger of Oracle, change it to lang

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->

<class 
    name="agtc.sampletracking.model.Run" 
    table="Run"
>

    <id
        name="runId"
        type="java.lang.Integer"
        column="RUN_ID"
        unsaved-value="-1"
    >
  
       	<generator class="native" />

    </id>

    <property
        name="plateList"
        type="java.lang.String"
        column="PLATE_LIST"
        length="256"
    >
    </property>
    
    <property
        name="testList"
        type="java.lang.String"
        column="TEST_LIST"
        length="256"
    >
    </property>
    <property
        name="note"
        type="java.lang.String"
        column="NOTE"
        length="128"
       
    >
    </property>
    
   <property
        name="runDate"
        type="java.sql.Timestamp"
        column="RUN_DATE"
        length="7"
    >
   
    </property>
    
   <!-- bi-directional many-to-one association to Project -->
    <many-to-one
        name="project"
        class="agtc.sampletracking.model.Project"
    >
        <column name="PROJECT_ID" />
    </many-to-one>
    
    <!-- bi-directional one-to-many association to Result -->
    <set
        name="results"
        lazy="true"
        inverse="true"
        cascade="all"
    >
     
        <key>
            <column name="RUN_ID" />
        </key>
        <one-to-many 
            class="agtc.sampletracking.model.Result"
        />
    </set>

</class>
</hibernate-mapping>