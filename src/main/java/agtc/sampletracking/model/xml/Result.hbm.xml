<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->

<class 
    name="agtc.sampletracking.model.Result" 
    table="RESULT"
>
  

    <id
        name="resultId"
        type="java.lang.Integer"
        column="RESULT_ID"
        unsaved-value="-1"
    >
    	<generator class="native" />
    </id>

    <property
        name="result"
        type="java.lang.String"
        column="RESULT"
        length="10"
    >
    </property>
    <property
        name="note"
        type="java.lang.String"
        column="NOTE"
        length="64"
    >
      
    </property>
    
    <property
        name="studyGroupId"
        type="java.lang.Integer"
        column="STUDY_GROUP_ID"
    >
      
    </property>

	<property
        name="intSampleId"
        type="java.lang.String"
        column="INT_SAMPLE_ID"
        length="16"
    >
       
    </property>

   <!--
	<property
        name="useThis"
        type="java.lang.String"
        column="USE_THIS"
        length="1"
    >
      
    </property>

	<property
        name="export2CGG"
        type="java.lang.String"
        column="EXPORT2CGG"
        length="1"
    >
      
    </property>
	-->

    <!-- associations -->
   
   
    <!-- bi-directional many-to-one association to Assay -->
    <many-to-one
        name="assay"
        class="agtc.sampletracking.model.Assay"
        not-null="true"
    >
        <column name="ASSAY_ID" />
    </many-to-one>

   <!-- bi-directional many-to-one association to Run -->
    <many-to-one
        name="run"
        class="agtc.sampletracking.model.Run"
        not-null="true"
    >
        <column name="RUN_ID" />
    </many-to-one>
</class>
</hibernate-mapping>
