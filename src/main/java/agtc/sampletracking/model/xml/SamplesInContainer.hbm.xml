<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<!-- Created by the Middlegen Hibernate plugin modified by Gloria Deng generate 
		long mapping for longerger of Oracle, change it to lang http://boss.bekk.no/boss/middlegen/ 
		http://hibernate.sourceforge.net/ -->

	<class name="agtc.sampletracking.model.SamplesInContainer" table="SAMPLES_IN_CONTAINER">

		<id name="sicId" type="java.lang.Integer" column="SICID"
			unsaved-value="-1">

			<generator class="native" />

		</id>

		<property name="well" type="java.lang.String" column="LOCATION"
			length="6">
		</property>

		<property name="operation" type="java.lang.String" column="OPERATION"
			length="1">
		</property>

		<property name="operationDate" type="java.sql.Timestamp"
			column="OPERATIONDATE">

		</property>

		<property name="reason" type="java.lang.String" column="REASON"
			length="126">
		</property>

		<!-- bi-directional many-to-one association to Sample -->
		<many-to-one name="sample" class="agtc.sampletracking.model.Sample">
			<column name="SAMPLE_ID" not-null="true" />
		</many-to-one>

		<!-- bi-directional many-to-one association to Container -->
		<many-to-one name="container" class="agtc.sampletracking.model.Container">
			<column name="CONTAINER_ID" not-null="true" />
		</many-to-one>


	</class>
</hibernate-mapping>